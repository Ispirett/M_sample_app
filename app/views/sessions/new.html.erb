<%= provide :title, "SignIn" %>
<h1>Sign IN</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "login", class: "btn btn-primary" %>
    <% end %>
    <%= link_to 'logout', logout_path, method: :delete %>
  </div>
</div>